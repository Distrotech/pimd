[project]
	description = "This is the original USC pimd, revived at github to serve as  a central point of collaboration for all in need of a small and efficient PIM-SM implementation.  Thanks to OpenBSD pimd is now free software under the 3-clause BSD license!"
	state = read only
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Homer Simpson (gregnietsky)
[access "refs/for/refs/*"]
	read = group user/Homer Simpson (gregnietsky)
	push = group user/Homer Simpson (gregnietsky)
	label-Code-Review = -2..+2 group user/Homer Simpson (gregnietsky)
	label-Verified = -1..+1 group user/Homer Simpson (gregnietsky)
	rebase = group user/Homer Simpson (gregnietsky)
	removeReviewer = group user/Homer Simpson (gregnietsky)
	submit = group user/Homer Simpson (gregnietsky)
[access "refs/heads/*"]
	read = group user/Homer Simpson (gregnietsky)
	create = group user/Homer Simpson (gregnietsky)
	push = +force group user/Homer Simpson (gregnietsky)
	pushMerge = group user/Homer Simpson (gregnietsky)
[access "refs/tags/*"]
	push = +force group user/Homer Simpson (gregnietsky)
	pushTag = group user/Homer Simpson (gregnietsky)
	pushSignedTag = group user/Homer Simpson (gregnietsky)
